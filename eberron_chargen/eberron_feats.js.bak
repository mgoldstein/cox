function lock_feats() { 
	if (FeatsAreLocked==true) { 
		document.form1.sixth_comment_box.value="Feats are already assigned.\r";
		FinalSuggestions();
		return;
	}
	if (Feats()) { 
		AnimalCompanion();
		Warhorse();
		QuoriSpirit();
		if ((eberronprestigeclassnumber==14) || (has_dragonprophesier)) { document.form1.speak_draconic.checked=true; }
		if ((eberronprestigeclassnumber==41)) { document.form1.speak_auran.checked=true; }
		if ((eberronprestigeclassnumber==42)) { document.form1.speak_terran.checked=true; }
		if ((eberronprestigeclassnumber==43)) { document.form1.speak_ignan.checked=true; }
		if ((eberronprestigeclassnumber==44)) { document.form1.speak_aquan.checked=true; }
		if (has_education==true) { 
			AllKnowledgeSkills();
		}
		if (has_wardeninitiate==true) { 
			climb_class=1;
			jump_class=1;
		}
		if (has_greensingerinitiate==true) { 
			bluff_class=1;
			hide_class=1;
			perform_1_class=1;
			perform_2_class=1;
			perform_3_class=1;
		}
		if (has_nightbringerinitiate==true) { 
			hide_class=1;
			movesilently_class=1;
	 	}
		if (has_markoftwelvemoonsstrength) { strength++; }
		if (has_markoftwelvemoonsconstitution) { constitution++; }
		if (has_markoftwelvemoonsdexterity) { dexterity++; }
		if (has_markoftwelvemoonswisdom) { wisdom++; }
		if (has_markoftwelvemoonsintelligence) { intelligence++; }
		if (has_markoftwelvemoonscharisma) { charisma++; }
		// FinalSuggestions();
		PutMeOnTheMap();
		BasicEyeColor();
		BasicSkinColor();
		BasicHairColor();
		pigmentation();
		ClassNeeds();
		Arms();
		FeatsAreLocked=true;
		AdjustHeight();
		AdjustWeight();
		Languages();
		document.form1.sixth_comment_box.value="Feats are assigned.\r";
		FinalSuggestions();
	} else { 
		document.form1.sixth_comment_box.value="Continue working on your feats.";
	}
	ArrowShow();
}