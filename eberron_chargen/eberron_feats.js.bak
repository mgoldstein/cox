	if (vadalisbeastkeeperfeatentitlement>0) { document.form1.feat.value+="There must be "+vadalisbeastkeeperfeatentitlement+" Vadalis beastkeeper (VB).  You have "+total_vadalisbeastkeeper_feats+".\r"; }
