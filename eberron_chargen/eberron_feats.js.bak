	if (eberronprestigeclassnumber==11) { document.form1.feat.value+="An archmage is expected to have Spell Focus in two schools of magic.  You have "+archmage_spell_foci+".\r"; }
	if (eberronprestigeclassnumber==18) { document.form1.feat.value+="A heirophant is expected to have a metamagic feat.  You have "+heirophant_metamagic+".\r"; }
	if (eberronprestigeclassnumber==61) { document.form1.feat.value+="An escalation mage is expected to have a metamagic feat.\r"; }
	if (eberronprestigeclassnumber==20) { document.form1.feat.value+="A loremaster is expected to have three metamagic / item creation feats.  You have "+loremaster_1+".\r"; }
	if (eberronprestigeclassnumber==20) { document.form1.feat.value+="A loremaster is expected to have skill focus in a knowledge skill.  You have "+loremaster_2+".\r"; }
	if (eberronprestigeclassnumber==33) { document.form1.feat.value+="A quori nightmare is expected to have at least one psionic feat.\r"; }
// PRESTIGECLASSSTOP
	if (eberronprestigeclassnumber==35) { document.form1.feat.value+="A recaster is expected to have at least two metamagic feats.\r"; }
	if (eberronprestigeclassnumber==66) { document.form1.feat.value+="A primal scholar should have at least one metamagic feat.\r"; }
	if (eberronprestigeclassnumber==62) { document.form1.feat.value+="An argent fist may check monastic training and then un-check knight training.\r"; }
